<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.streama" version="0.0.1" name="TVNETPlay" provider-name="www.tvnetplay.net">
    <requires>
      <import addon="xbmc.python" version="2.24.0"/>
      <import addon="script.module.simplejson" version="3.3.0"/>
      <import addon="script.module.requests" version="2.3.0"/>
      <import addon="script.module.routing" version="0.2.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="addon.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en_GB">Servidor Multimedia.</summary>
        <description lang="en_GB">TVNETPlay es un Centro de Entretenimiento donde puedes disfrutar desde la comodidad de tu hogar de las mejores Pel√≠culas y Series de TV!
        </description>
        <platform>all</platform>
        <license>MIT</license>
        <source>https://github.com/HackerspaceBielefeld/plugin.video.streama</source>
        <website>http://github.com/HackerspaceBielefeld/plugin.video.streama</website>
        <forum>https://forum.kodi.tv/showthread.php?tid=320484</forum>
        <news></news>
        <assets>
            <icon>resources/media/icon.png</icon>
            <fanart>resources/media/fanart.jpg</fanart>
            <screenshot>resources/media/screenshot01.jpg</screenshot>
        </assets>
    </extension>
</addon>

<addon id="skin.eminence.2"  version="3.0.4" name="Eminence" provider-name="jurialmunkey">
    <requires>
        <import addon="xbmc.gui" version="5.14.0" />
        <import addon="script.skinshortcuts" version="1.0.0" />
        <import addon="script.skin.helper.service" version="1.0.0" />
        <import addon="script.extendedinfo" version="5.0.0" />
        <import addon="script.image.resource.select" version="0.0.5" />
        <import addon="plugin.program.autocompletion" version="1.0.1" />
    </requires>
    <extension debugging="false" effectslowdown="1.00" point="xbmc.gui.skin">
        <res aspect="16:9" default="true" folder="16x9" height="1080" width="1920" />
    </extension>
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <summary lang="en_GB">Clean, clear, simple, modern.</summary></description>
        <license>CC-NC-SA 3.0</license>
        <website />
        <email />
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
            <screenshot>extras/viewthumbs/50.jpg</screenshot>
        </assets>
    </extension>
</addon>
</addons>
